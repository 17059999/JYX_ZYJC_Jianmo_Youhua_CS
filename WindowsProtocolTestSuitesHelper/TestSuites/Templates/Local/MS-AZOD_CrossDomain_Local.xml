<lab>
  <core>
    <username>administrator</username>
    <password>Password01!</password>
    <legacyclient>true</legacyclient>
    <basecpu>2</basecpu>
    <TestSuiteName>MS-AZOD_CrossDomain</TestSuiteName>
    <Description>DC x 2 and Member x 4</Description>
    <regressiontype>local</regressiontype>
    <timeoutinminutes>230</timeoutinminutes>
  </core>
  <network>
    <vnet>
       <name>MS-AZOD-Internal</name>
      <networktype>internal</networktype>
      <hostisgateway>true</hostisgateway>
      <ip>192.168.50.254</ip>
      <subnet>255.255.255.0</subnet>
    </vnet>
  </network>
  
  <servers>
    <vm>
      <installorder>1</installorder>
      <hypervname>MS-AZOD_DC01</hypervname>
      <name>DC01</name>
      <role>localdomaincontroller</role>
      <imageReference>
        <diskName>17763.194.amd64fre.rs5_release.VS2017-ServerDataCenter.vhd</diskName>
        <answerFile>17763_ServerDataCenter.xml</answerFile>
      </imageReference>
      <domain>contoso.com</domain>        
      <adminname>administrator</adminname>   
      <adminpassword>Password01!</adminpassword>   
      <vnet>MS-AZOD-Internal</vnet>
      <ip>192.168.50.1</ip> 
      <gateway>192.168.50.254</gateway>
      <dns>127.0.0.1;192.168.50.10</dns>
      <memory>1024</memory>      
      <installscript>createdc.ps1</installscript> 	  	  
      <postscript>Disable_Firewall.ps1;SetPasswordNeverExpires.ps1;InstallMSIAndTools.ps1;Write-InstallDoneSignal.ps1;UpdateConfigFile.ps1;Config-DC01.ps1</postscript>
	  <tools>        
        <TestsuiteMSI MSIName="MS-AZOD-TestSuite-ODEP.msi" targetEndpoint="SUT" />
      </tools>
    </vm>
    
     <vm>
      <installorder>2</installorder>
      <hypervname>MS-AZOD_DC02</hypervname>
      <name>DC02</name>
      <role>trustdomaincontroller</role>
       <imageReference>
         <diskName>17763.194.amd64fre.rs5_release.VS2017-ServerDataCenter.vhd</diskName>
         <answerFile>17763_ServerDataCenter.xml</answerFile>
       </imageReference>
      <domain>kerb.com</domain>
      <adminname>administrator</adminname>   
      <adminpassword>Password01!</adminpassword> 
      <vnet>MS-AZOD-Internal</vnet>
      <ip>192.168.50.10</ip>
      <gateway>192.168.50.254</gateway>
      <dns>127.0.0.1;192.168.50.1</dns>
      <memory>1024</memory>
      <installscript>createdc.ps1</installscript> 	  	  
      <postscript>Disable_Firewall.ps1;SetPasswordNeverExpires.ps1;InstallMSIAndTools.ps1;Write-InstallDoneSignal.ps1;UpdateConfigFile.ps1;Config-DC02.ps1</postscript>
      <tools>
        <TestsuiteMSI MSIName="MS-AZOD-TestSuite-ODEP.msi" targetEndpoint="SUT" />
      </tools>
    </vm>

    <vm>
      <installorder>3</installorder>
      <hypervname>MS-AZOD_AP01</hypervname>
      <name>AP01</name>
      <role>localap01</role>
      <imageReference>
        <diskName>17763.194.amd64fre.rs5_release.VS2017-ServerDataCenter.vhd</diskName>
        <answerFile>17763_ServerDataCenter.xml</answerFile>
      </imageReference>
      <domain>contoso.com</domain>
      <vnet>MS-AZOD-Internal</vnet>
      <ip>192.168.50.2</ip>
      <gateway>192.168.50.254</gateway>
      <dns>192.168.50.1;192.168.50.10</dns>
      <memory>1024</memory>   
      <installfeaturescript>Install-FSRM.ps1</installfeaturescript> 
      <installscript>domainjoin.ps1</installscript>        
      <postscript>Disable_Firewall.ps1;SetPasswordNeverExpires.ps1;InstallMSIAndTools.ps1;Write-InstallDoneSignal.ps1;UpdateConfigFile.ps1;Config-AP01.ps1</postscript>
      <tools>        
        <TestsuiteMSI MSIName="MS-AZOD-TestSuite-ODEP.msi" targetEndpoint="SUT" />
      </tools>
    </vm>   
    <vm>
      <installorder>4</installorder>
      <hypervname>MS-AZOD_AP02</hypervname>
      <name>AP02</name>
      <role>trustap01</role>
      <imageReference>
        <diskName>17763.194.amd64fre.rs5_release.VS2017-ServerDataCenter.vhd</diskName>
        <answerFile>17763_ServerDataCenter.xml</answerFile>
      </imageReference>
      <domain>kerb.com</domain>      
      <vnet>MS-AZOD-Internal</vnet>
      <ip>192.168.50.11</ip>
      <gateway>192.168.50.254</gateway>
      <dns>192.168.50.10;192.168.50.1</dns>
      <memory>1024</memory>
      <installfeaturescript>Install-FSRM.ps1</installfeaturescript>
      <installscript>domainjoin.ps1</installscript>  
      <postscript>Disable_Firewall.ps1;SetPasswordNeverExpires.ps1;InstallMSIAndTools.ps1;UpdateConfigFile.ps1;config-ap02.ps1</postscript>
      <tools>
        <TestsuiteMSI MSIName="MS-AZOD-TestSuite-ODEP.msi" targetEndpoint="SUT" />
      </tools>
    </vm>
    <vm>
      <sku>client</sku>	  
      <installorder>5</installorder>
      <hypervname>MS-AZOD_CLIENT01</hypervname>
      <name>CLIENT01</name>
      <domain>contoso.com</domain>      
      <username>Administrator</username>
	    <password>Password01!</password>
      <role>DriverComputer</role>
      <imageReference>
        <diskName>17763.194.amd64fre.rs5_release.VS2017-ServerDataCenter.vhd</diskName>
        <answerFile>17763_ServerDataCenter.xml</answerFile>
      </imageReference>
      <vnet>MS-AZOD-Internal</vnet>
      <ip>192.168.50.3</ip>
      <gateway>192.168.50.254</gateway>
      <dns>192.168.50.1;192.168.50.11</dns>
      <memory>2048</memory>
      <installscript>domainjoin.ps1</installscript>    
      <postscript>Disable_Firewall.ps1;SetPasswordNeverExpires.ps1;InstallMSIAndTools.ps1;UpdateConfigFile.ps1;Config-Client01.ps1;Update-VisualStudioLicense.ps1</postscript>
      <skipwaitingforpostscript>false</skipwaitingforpostscript>
      <tools>
        <tool name="SE" MSIName="SpecExplorer.msi" version="3.5.3146.0" CPUArchitecture="x86" />
        <tool name="PTF" MSIName="ProtocolTestFrameworkInstaller.msi" version="1.0.7500.0" CPUArchitecture="x86" />
		    <tool name="MMA" MSIName="MessageAnalyzer64.msi" version="1.20.0107.01" CPUArchitecture="x64" /> 
        <TestsuiteMSI MSIName="MS-AZOD-TestSuite-ODEP.msi" targetEndpoint="TESTSUITE" />
      </tools>
    </vm>
  </servers>  
</lab>
