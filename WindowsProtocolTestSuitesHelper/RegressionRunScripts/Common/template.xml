        <Parameter Name="TestDirInVM" Value="C:\Test" Comment="" />
    </Parameters>
    <servers>
        <vm>
            <role>NODE01</role>
            <installorder>1</installorder>
            <!-- if you're using your customized VHD, please refer to DriverComputer-->
            <imageReference>
                <!-- publisher                  offer           sku                                 -->
                <!-- MicrosoftWindowsDesktop    Windows-10      RS5-Pro                             -->
                <!-- MicrosoftWindowsDesktop    Windows-10      rs4-pro                             -->
                <!-- MicrosoftWindowsDesktop    Windows-10      RS3-Pro                             -->
