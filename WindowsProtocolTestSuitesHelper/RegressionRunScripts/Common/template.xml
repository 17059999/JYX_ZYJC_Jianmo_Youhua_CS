                <version>latest</version>
            </imageReference>
            <name>SMB2-SRV01</name>
            <domain>SNIA-12R2.org</domain>
            <ip>10.0.1.61</ip>
            <ip>10.0.1.62</ip>
            <dns>10.0.1.60</dns>
            <!-- make sure write a signal file after installscript/installfeaturescript/postscript/configscript completed -->
            <!-- NodeName               SignalName                              -->
            <!-- installscript          Install.Completed.signal                -->
            <!-- installfeaturescript   InstallFeatureScript.Completed.signal   -->
            <!-- postscript             PostScript.Completed.signal             -->
            <!-- configscript           ConfigScript.Completed.signal           -->
            <installfeaturescript>Install-WindowsFeature.ps1;Write-InstallFeatureScirptDone.ps1;RestartComputer.PS1</installfeaturescript>
            <postscript>PostConfig-STDNode01.ps1</postscript>
        </vm>

        <vm>
            <role>DriverComputer</role>
            <installorder>5</installorder>
            <!-- if you're using Azure provider VHD, please refer to NODE01-->
